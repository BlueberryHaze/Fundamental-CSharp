using System;

namespace Demo
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = int.Parse(Console.ReadLine());
            int b = int.Parse(Console.ReadLine());
            int total = 0;

            for (int i = a; i <= b; i++)
            {
                total += i;
                Console.Write($" {i}");

            }
            Console.WriteLine();
            Console.WriteLine($"Sum: {total}");
        }

    }
}
